{
  "name": "angular-form-generator",
  "description": "Dynamic drag and drop angular-js form creation, utilizing FontAwesome",
  "main": [
    "dist/angular-form-gen.js",
    "dist/angular-form-gen.css"
  ],
  "license": "MIT",
  "homepage": "https://github.com/adrieltiti/angular-form-gen",
  "repository": {
    "type": "git",
    "url": "git@github.com:adrieltiti/angular-form-gen.get"
  },
  "keywords": [
    "angularjs",
    "angular",
    "form",
    "schema",
    "generator"
  ],
  "version": "0.3.0",
  "authors": [
    "Null McNull <noreplies@hotmail.com> https://github.com/McNull",
    "Pranav Sathy https://github.com/PranavSathy",
    "Adriel Acosta https://github.com/adrieltiti",
    "Alien Fernandez <alienfernandez85@gmail.com> https://github.com/alienfernandez",
  ],
  "moduleType": [
    "globals"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "gulp-tasks",
    "src",
    "dest",
    "projectFilesBackup",
    "/build-config.js",
    "/gulpfile.js",
    "/package.json",
    "/karma.conf.js"
  ],
  "dependencies": {
    "angular": "^1.3.0",
    "bootstrap": "~3.3.4"
  },
  "devDependencies": {
    "angular-route": "~1.2.21",
    "angular-mocks": "~1.2.21",
    "jquery": "1.*",
    "respond": "~1.4.2",
    "html5shiv": "~3.7.2",
    "lodash": "~2.4.1",
    "angular-inform": "~0.0.12",
    "angular-block-ui": "~0.1.0",
    "angular-markdown-text": "~0.0.2",
    "angular-sanitize": "~1.2.21",
    "angular-animate": "~1.2.21",
    "bootstrap-daterangepicker":"2.1.25"
  },
  "overrides": {
    "respond": {
      "main": "dest/respond.min.js"
    },
    "angular-mocks": {
      "ignore": true
    },
    "lodash": {
      "ignore": true
    },
    "showdown": {
      "main": "compressed/showdown.js"
    }
  },
  "resolutions": {
    "angular": "^1.3.0"
  }
}
